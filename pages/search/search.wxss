.main{
    padding: 0 20rpx;
    background-color: #0094aa;   
    width:750rpx;
    height: 118rpx; 
    position: relative;
    box-sizing: border-box;
    overflow-x:hidden;
}
/* 搜索盒子 */
.search-box {
    position: relative;
    display: flex;
    justify-content: space-between;
    padding-top: 26rpx;
}
/* 输入框 */
.search-input {
    position: relative;
    width: 700rpx;
    height: 60rpx;
    line-height: 60rpx;
    padding: 0rpx 20rpx 0rpx 20rpx;
    background: #f8f8f8;
    border-radius: 10rpx;
    font-size: 32rpx;
    border: 2rpx solid #ddd;
}
/* 搜索取消图标 */
.search-icon {
    position: relative;
    left: -60rpx;
    top: 8rpx;
    width: 35rpx;
    height: 35rpx;
}
/* 取消 */
.search-btn{
    width: 90rpx;
    position: relative;
    left: 20rpx;
    font-size: 30rpx;
    line-height: 64rpx;
}
/* 返回键 */
.back_Icon{
    float: left;
    height: 60rpx;
    width: 60rpx;
    position: relative;
    top:26rpx
}
/* 标题框架 */
.title{
    font-size: 32rpx;
    margin: 50rpx 0 30rpx 0rpx;
}
/* 标题文本 */
.title text{
    padding-right: 30rpx;
    padding-bottom: 10rpx;
    color: #a9c3e7;
    border-bottom: 8rpx ;
}
/* 清除键 */
.title text.clear{
    color:#bfbfbf ;
    padding-left: 450rpx;
}
/* 标签 */
.tags text{
    display: inline-block;
    font-size: 28rpx;
    padding: 6rpx 20rpx;
    border: 2rpx solid #ddd;
    color: #aaa;
    margin: 0 30rpx 20rpx 0;
    border-radius: 40rpx;
    word-break: break-all;
}
/* 显示关键词框架 */
.keywords{
    position: absolute;
    width: 100%;
    left: 0;
    top: 120rpx;
    background: #fff;
    z-index: 9;
    padding: 20rpx;
    box-sizing: border-box;
}
/* 关键词条框架 */
.keywords-item{
    line-height: 70rpx;
    border-bottom: 1px solid #eee;
    color: #787878;
    font-size: 28rpx;
}
/* 结果大体框架 */
.result{
    margin-top: 0rpx;
}
/* 结果大体框架 */
.result-box, .recommend-box{
    padding:0 20rpx;
}
/* 每项结果的框架 */
.result-list{
    display: inline-block;
    width: 325rpx;
    height: 325rpx;
    margin:0 20rpx 20rpx 0;
    border-radius: 10px;
    text-align: center;
    background: #f5f6f5;
}
/* 最后偶数结果框架 */
.result-list:nth-child(2n){
    margin-right: 0;
}
/* 结果图片 */
.result-list image{
    width: 175rpx;
    height: 175rpx;
    margin: 20rpx 0 10rpx;
}
/* 文字大小 */
.result-list .result-text{
    font-size: 32rpx;
}
/* 搜索为空时候框架 */
.null-tips{
    width: 100%;
    padding: 200rpx 0;
    text-align: center;
    color: #787878;
}

/* 相关推荐 */
.recommend-title{
    position: relative;
    height: 0;
    width: 100%;
    border-bottom: 1px solid #f0f0f1;
    margin-bottom: 42rpx;
  }
  /* 相关推荐 */
  .recommend-title text{
    position: absolute;
    left: 50%;
    top: -20rpx;
    width: 150rpx;
    height: 40rpx;
    margin-left: -75rpx;
    line-height: 40rpx;
    background: #ffffff;
    color: #5e5e5e;
    font-size: 28rpx;
    font-weight: bold;
    text-align: center;
    letter-spacing: 1px;
  }
  /* 热点搜索和历史搜索框架 */
  .init-page{
    background-color: white;
    border-radius:5px ;
  }
 /* 文章框 */
 .item_show{
    position: relative;
    width: 100%;
    border:none;
    border-bottom: 2px solid #dcdcdc;
    overflow-x:hidden;
}
/* 文章标题 */
.item_title{
    color: #0366d6;
    font-size: 44rpx;
    position: relative;
    overflow: hidden;
    top: 15rpx;
    left: 30rpx;
    width: 500rpx;
}
.focks{
    position: relative;
    overflow: hidden;
    top: 20rpx;
    left: 30rpx;
}
.star{
    margin-right: 20rpx;
    float: left;
    position: relative;
    overflow: hidden;
    top: 20rpx;
    left: 30rpx;
}
/* 内容简介 */
.content{
    color: gray;
    font-size: 30rpx;
    position: relative;
    overflow: hidden;
    top:20rpx;
    width: 690rpx;
    left: 30rpx;
}
/* 收藏 */
.collect
{
    position:absolute;
    right: 30rpx;
    height: 60rpx;
    width: 25px;
    height: 25px;
    top:25rpx;
}
/* 来源 */
.from{
  position: relative;
  top: 80rpx;
  left: 75rpx;
  float:left;
  overflow: hidden;
}
/* 平台 */
.point_Num
{
    margin-top: 20rpx;
    margin-bottom: 15rpx;
    position: relative;
    top: 10rpx;
    left: 30rpx;
}
/* 文章点击量 */
.deskTop
{   
    color: green;
    left: 30rpx;
    position: relative;
}
/* 心形 */
.like{
  position: absolute;
  float: left;
  top: 250rpx;
  left: 600rpx;
  width: 50rpx;
  height: 50rpx;
}
/* 心形图片 */
.like_Image{
    width: 100%;
    height: 100%;
}
 /* 加载更多   */
.weui-loading {
    margin: 0 5px;
    width: 20px;
    height: 20px;
    display: inline-block;
    vertical-align: middle;
    -webkit-animation: weuiLoading 1s steps(12, end) infinite;
    animation: weuiLoading 1s steps(12, end) infinite;
    background: transparent url(data:image/svg+xml;base64,) no-repeat;
    background-size: 100%;
  }
  .weui-loadmore {
    width: 65%;
    margin: 1.5em auto;
    line-height: 1.6em;
    font-size: 14px;
    text-align: center;
  }
  .weui-loadmore__tips {
    display: inline-block;
    vertical-align: middle;
  }
  /* 顶部 */
.top{
    width: 100vw;
    height: 80rpx;
    padding: 0 20rpx;
    line-height: 80rpx;
    font-size: 34rpx;
    border-radius: 20rpx;
    border: 1px solid gray;
   }
   .top-text{
    float: left
   }
   /* 下拉框 */
   .top-selected{
    border-radius: 10rpx;
     width: 50%;
     display: flex;
     float: right;
     align-items: center;
     justify-content: space-between;
     border: 1px solid #ccc;
     border-bottom: none;
     padding: 0 10rpx;
     font-size: 30rpx;
   }
   /* 下拉内容 */
   .select_box {
    background-color: #fff;
    padding: 0 20rpx;
    width: 45%;
    float: right;
    position: absolute;
    top: 200rpx;
    right: 0;
    z-index: 1;
    overflow: hidden;
    text-align: left;
    animation: myfirst 0.5s;
    font-size: 30rpx;
  }
  .select_one {
   border-radius: 10rpx;
    padding-left: 20rpx;
    width: 100%;
    height: 60rpx;
    position: relative;
    line-height: 60rpx;
    border: 1px solid #ccc;
  }
   /* 下拉过度效果 */
   @keyframes myfirst {
     from {
       height: 0rpx;
     }
    
     to {
       height: 110rpx;
     }
   }
   /* 下拉图标 */
   .top-selected image{
    height:50rpx;
    width:50rpx;
    position: absolute;
    right: 0rpx;
    top: 20rpx;
   }
   .select-icon{
     position: relative;
     top: -45rpx;
   }